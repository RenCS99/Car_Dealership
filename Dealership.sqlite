CREATE TABLE Employee (
    e_eid INT AUTOINCREMENT,
    e_did INT NOT NULL,
    e_admin CHAR(1) NOT NULL,
    e_inventoryId INT NOT NULL,
    PRIMARY KEY (e_eid),
    FOREIGN KEY (e_did) REFERENCES Dealer(e_did)
);

CREATE TABLE Dealer (
    d_did INT AUTOINCREMENT,
    d_name STRING NOT NULL,
    d_location STRING NOT NULL,
    d_eid INT NOT NULL,
    PRIMARY KEY (d_did),
    FOREING KEY (d_eid) REFERENCES Employee(e_eid)
);

CREATE TABLE Inventory (
    i_inventoryId INT AUTOINCREMENT,
    i_name STRING NOT NULL,
    i_location STRING NOT NULL,
    i_did INT NOT NULL,
    i_modelName STRING NOT NULL,
    i_modelYear INT NOT NULL,
    i_brandName STRING NOT NULL,
    i_vin VARCHAR(17) NOT NULL,
    PRIMARY KEY (i_inventoryId),
    FOREIGN KEY (i_did) REFERENCES Dealer(d_did),
    FOREIGN KEY (i_modelName) REFERENCES Car_Model(cm_modelName),
    FOREIGN KEY (i_modelYear) REFERENCES Car_Model(cm_modelYear),
    FOREIGN KEY (i_brandName) REFERENCES Car_Model(cm_brandModel),
    FOREIGN KEY (i_vin) REFERENCES Vehicle(m_vin)
);

CREATE TABLE Vehicle (
    v_vid INT AUTOINCREMENT,
    v_cid INT NOT NULL,
    v_oid INT NOT NULL,
    v_vin VARCHAR(17) NOT NULL,
    v_price FLOAT NOT NULL,
    v_inventoryId INT NOT NULL,
    v_manufacturerId INT NOT NULL,
    PRIMARY KEY (v_vid),
    FOREING KEY (v_cid) REFERENCES Customer(c_cid),
    FOREING KEY (v_oid) REFERENCES Options(o_oid),
    FOREING KEY (v_inventoryId) REFERENCES Inventory(i_inventoryId),
    FOREING KEY (v_manufacturerId) REFERENCES Manufacturer(m_manufacturerId)
);

CREATE TABLE Sales (
    s_sid INT AUTOINCREMENT,
    s_eid INT NOT NULL,
    s_did INT NOT NULL,
    s_cid INT NOT NULL,
    s_vin VARCHAR(17) NOT NULL,
    s_date DATE,
    s_price FLOAT NOT NULL,
    PRIMARY KEY (s_sid),
    FOREING KEY (s_eid) REFERENCES Employee(e_eid),
    FOREING KEY (s_did) REFERENCES Dealer(d_did),
    FOREING KEY (s_cid) REFERENCES Customer(c_cid),
    FOREING KEY (s_vin) REFERENCES Vehicle(v_vin),
    FOREING KEY (s_price) REFERENCES Vehicle(v_price)
);

CREATE TABLE Customer (
    c_cid INT AUTOINCREMENT,
    c_did INT NOT NULL,
    c_name STRING NOT NULL,
    c_phone VARCHAR(12) NOT NULL,
    c_gender CHAR(1),
    PRIMARY KEY (c_cid),
    FOREING KEY (c_did) REFERENCES Dealer(d_did),
);

CREATE TABLE Manufacturer (
    m_manufacturerId INT AUTOINCREMENT,
    m_vin VARCHAR(17) NOT NULL,
    m_manufacturerName STRING NOT NULL,
    m_location STRING NOT NULL,
    PRIMARY KEY (m_manufacturerId),
    FOREING KEY (m_vin) REFERENCES Vehicle(v_vin)
);

CREATE TABLE Options (
    o_oid INT AUTOINCREMENT,
    o_manufacturerId INT NOT NULL,
    o_transmission CHAR(3) NOT NULL,
    o_engine STRING NOT NULL,
    o_color VARCHAR(1) NOT NULL,
    o_cmid INT NULL,
    PRIMARY KEY (o_oid),
    FOREING KEY (o_manufacturerId) REFERENCES Manufacturer(m_manufacturerId),
    FOREING KEY (o_cmid) REFERENCES Car_Model(cm_cmid)
);

CREATE TABLE Car_Model (
    cm_cmid INT AUTOINCREMENT,
    cm_bodyStyle STRING NOT NULL,
    cm_manufacturerName STRING NOT NULL,
    cm_modelName STRING NOT NULL,
    cm_modelYear INT(4) NOT NULL,
    cm_brandName STRING NOT NULL,
    PRIMARY KEY (cm_cmid),
    FOREING KEY (cm_manufacturerName) REFERENCES Manufacturer(m_manufacturerName)
);